<div class="col-md-12">
<%= bootstrap_form_for [@item, @itemquestion], remote:true do |f| %>

  <%= f.text_field :name, label:"Name" %>
  <%= f.text_field :email, label:"Email" %>
  <%= f.text_area :message, label:"Message" %>
  <div class="buttonz">
    <%= f.submit "Inquire", class:"btn button-black" %>
  </div>

<% end %>
<br>
</div>
