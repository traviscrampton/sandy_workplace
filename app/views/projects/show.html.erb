<div class="container">
  <div class="col-md-7 project_form">
    <% @project.photos.each do |photo| %>
      <%= image_tag photo.pic.url(:large) %>
    <% end %>
  </div>
  <div class="col-md-4">
    <h3><%= @project.title %></h3>
    <p> <%= @project.description %> </p>
    <%= link_to "Add Photo", new_project_photo_path(@project), id:"linker", remote:true %>
  </div>


</div>
